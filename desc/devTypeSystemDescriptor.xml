        </featureDescription>
        <featureDescription>
          <name>relationSubType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.language.PhraseChunk</name>
