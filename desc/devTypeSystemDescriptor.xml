          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>dev.amb.uima.typeSystem.language.WordToken</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>headPhrase</name>
          <description/>
          <rangeTypeName>dev.amb.uima.typeSystem.language.PhraseChunk</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.language.CorefMention</name>
      <description/>
