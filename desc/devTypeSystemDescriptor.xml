<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>devTypeSystemDescriptor</name>
  <description>Type system for relation extraction.

Includes language annoations for WordTokens, PhraseChunks and Sentences and CorefMentions

Semantic type sinclude entities: Person, Location, Organistation, Date, Time and Relationship

MetaData includes a DocInfo key value annotation</description>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.language.Sentence</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>documentOrder</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>wordTokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>dev.amb.uima.typeSystem.language.WordToken</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>phraseChunks</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>dev.amb.uima.typeSystem.language.PhraseChunk</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>entityPairs</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>dev.amb.uima.typeSystem.semantic.EntitySentencePair</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.language.WordToken</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>startIndex</name>
          <description>Start index in input text</description>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endIndex</name>
          <description>end index in input text</description>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>sourceStartIndex</name>
          <description>The absolute start index in the source document - used when processing multiple sentences to store index position of word token in source document</description>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>sourceEndIndex</name>
          <description>Absolute end index of token in source document - used when processing multiple sentences to store index position of word token in source document</description>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>partOfSpeech</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>sentenceOrder</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.semantic.Entity</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
