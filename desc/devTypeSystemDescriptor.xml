          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>mentionLevel</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>aceUid</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
